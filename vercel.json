{
  "buildCommand": "next build",
  "devCommand": "next dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["bom1"],
  "outputDirectory": ".next",
  "rewrites": [
    {
      "source": "/p/:id",
      "destination": "/products/:id"
    },
    {
      "source": "/proj/:id",
      "destination": "/projects/:id"
    }
  ]
}